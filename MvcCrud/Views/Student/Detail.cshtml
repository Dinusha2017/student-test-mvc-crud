@model Student
@{
    ViewBag.Title = "Detail";
}

<h2>Detail</h2>

<a class="btn btn-primary" href="@Url.Action("Index", "Student")">
    <i class="fa fa-left-long"></i>
</a>

<div class="form-group row">
    @Html.LabelFor(model => model.StudentId, "Student ID", new { @class = "col-sm-2 detail-padding" })
    <div class="col-sm-10 detail-padding">@Html.DisplayFor(model => model.StudentId)</div>
</div>
<div class="form-group row">
    @Html.LabelFor(model => model.Name, new { @class = "col-sm-2 detail-padding" })
    <div class="col-sm-10 detail-padding">@Html.DisplayFor(model => model.Name)</div>
</div>
<div class="form-group row">
    @Html.LabelFor(model => model.City, new { @class = "col-sm-2 detail-padding" })
    <div class="col-sm-10 detail-padding">@Html.DisplayFor(model => model.City)</div>
</div>
<div class="form-group row">
    @Html.LabelFor(model => model.Address, new { @class = "col-sm-2 detail-padding" })
    <div class="col-sm-10 detail-padding">@Html.DisplayFor(model => model.Address)</div>
</div>
<div class="form-group row">
    @Html.LabelFor(model => model.Fees, new { @class = "col-sm-2 detail-padding" })
    <div class="col-sm-10 detail-padding">@Html.DisplayFor(model => model.Fees)</div>
</div>